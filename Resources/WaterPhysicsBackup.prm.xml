<?xml version="1.0" encoding="utf-8" standalone="no"?>
<Sonic.prm.xml>
  <Common>
    <Category>
      <Water>
        <Param>
          <AdditionalLimitTime>10</AdditionalLimitTime>
          <BuoyantForceMaxGravityRate>2</BuoyantForceMaxGravityRate>
          <BuoyantForceMaxVelocity>55</BuoyantForceMaxVelocity>
          <BuoyantForceZeroVelocity>5</BuoyantForceZeroVelocity>
          <LimitTimeToWaterWalk>32</LimitTimeToWaterWalk>
          <StartTimeToCountDown>21.5</StartTimeToCountDown>
          <WaterAddVelocity>6</WaterAddVelocity>
          <WaterDeadDownDepthMax>1.5</WaterDeadDownDepthMax>
          <WaterDeadDownDepthMaxSpeed>30</WaterDeadDownDepthMaxSpeed>
          <WaterDeadDownDepthMin>3</WaterDeadDownDepthMin>
          <WaterDecreaseForce>48.6</WaterDecreaseForce>
          <WaterDecreaseForceInDeepDepth>10</WaterDecreaseForceInDeepDepth>
          <WaterDrowningDepthMin>0</WaterDrowningDepthMin>
          <WaterDrowningNoBoostTime>0.2</WaterDrowningNoBoostTime>
          <WaterGravityScale>0.27</WaterGravityScale>
          <WaterTensionMaxVelocity>30</WaterTensionMaxVelocity>
          <WaterTensionZeroVelocity>0</WaterTensionZeroVelocity>
        </Param>
      </Water>
    </Category>
  </Common>
  <OnWater>
    <Category>
      <Boost>
        <Param>
          <BoostStartVelocityRate>1.2</BoostStartVelocityRate>
          <ROTATION_FORCE_IN_WATER_BOOST>35</ROTATION_FORCE_IN_WATER_BOOST>
        </Param>
      </Boost>
      <HomingAttack>
        <Param>
          <HomingAttackAfterUpVelocity>17</HomingAttackAfterUpVelocity>
        </Param>
      </HomingAttack>
      <Input>
        <Param>
          <PadInputForceCoeffInAir>0.55</PadInputForceCoeffInAir>
        </Param>
      </Input>
      <Jump>
        <Param>
          <JumpGroundVelocityXZRate>1</JumpGroundVelocityXZRate>
          <JumpGroundVelocityYRate>1</JumpGroundVelocityYRate>
          <JumpPower>10</JumpPower>
          <JumpPowerBySkill>10</JumpPowerBySkill>
          <JumpPowerDamageAir>10</JumpPowerDamageAir>
          <JumpPowerMin>5</JumpPowerMin>
          <JumpPowerMinOnBoard>5</JumpPowerMinOnBoard>
          <JumpPowerOnBoard>17</JumpPowerOnBoard>
          <KickbackPower>10</KickbackPower>
          <MaxUpSpeedWhenJump>10</MaxUpSpeedWhenJump>
        </Param>
      </Jump>
      <Other>
        <Param>
          <AttackAfterImpluseVelocityCoeff>0.55</AttackAfterImpluseVelocityCoeff>
          <SpringImpluseVelocityCoeff>1</SpringImpluseVelocityCoeff>
        </Param>
      </Other>
      <Rotation>
        <Param>
          <ROTATION_FORCE_IN_WATER>80</ROTATION_FORCE_IN_WATER>
        </Param>
      </Rotation>
      <Velocity>
        <Param>
          <AccelerationForce>12.5</AccelerationForce>
          <AccelerationForceBaseUp>25</AccelerationForceBaseUp>
          <AccelerationForceBySkill>25</AccelerationForceBySkill>
          <AccelerationForceSps>25</AccelerationForceSps>
          <MaxVelocityBasisBaseUp>40</MaxVelocityBasisBaseUp>
          <MaxVelocityBasisSps>145</MaxVelocityBasisSps>
        </Param>
      </Velocity>
    </Category>
  </OnWater>
</Sonic.prm.xml>